{
  "name": "osm2pdf",
  "version": "2.0.0",
  "description": "Generate pdf from OpenStreetMap tiles",
  "author": "mrkvon <mrkvon@protonmail.com> (https://mrkvon.org)",
  "repository": {
    "type": "git",
    "url": "git+https://git.mrkvon.org/mrkvon/osm2pdf.git"
  },
  "homepage": "https://git.mrkvon.org/mrkvon/osm2pdf",
  "main": "lib/index.js",
  "bin": "lib/run.js",
  "types": "lib/index.d.ts",
  "license": "MIT",
  "private": false,
  "preferGlobal": true,
  "keywords": [
    "osm",
    "openstreetmap",
    "pdf",
    "map",
    "maps",
    "offline-map"
  ],
  "scripts": {
    "test": "jest --config jestconfig.json",
    "build": "tsc",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "tslint -p tsconfig.json",
    "prepare": "yarn build",
    "prepublishOnly": "yarn test && yarn lint",
    "preversion": "yarn lint",
    "version": "yarn format && git add -A src",
    "postversion": "git push && git push --tags",
    "prestart": "yarn build",
    "start": "node lib/run.js"
  },
  "files": [
    "lib/**/*"
  ],
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/fs-extra": "^8.0.1",
    "@types/gm": "^1.18.7",
    "@types/jest": "^24.9.1",
    "@types/minimist": "^1.2.0",
    "@types/node": "^13.1.8",
    "@types/xml2js": "^0.4.5",
    "jest": "^25.1.0",
    "prettier": "^1.19.1",
    "ts-jest": "^25.0.0",
    "tslint": "^6.0.0",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.7.5"
  },
  "dependencies": {
    "axios": "^0.19.2",
    "fs-extra": "^8.1.0",
    "gm": "^1.23.1",
    "merge-img": "^2.1.3",
    "minimist": "^1.2.0"
  }
}
